    public static void createFrame() {
        JFrame frame = new JFrame("Student Tools");
        frame.setContentPane(new StudentToolsGUI().studentToolsPanel);
        frame.setDefaultCloseOperation(WindowConstants.EXIT_ON_CLOSE);
        frame.pack();
        frame.setLocationRelativeTo(null); //setting location to middle of user's screen.
        frame.setVisible(true);
    }

    public StudentToolsGUI() {
        $$$setupUI$$$();
        mainMenu.addActionListener(e -> {
            MainMenuGUI.createFrame();
            ((JFrame) studentToolsPanel.getTopLevelAncestor()).dispose(); //closing the current screen.
        });
        
        calculatorButton.addActionListener(e -> {
        MyGPA.createFrame();
        ((JFrame) studentToolsPanel.getTopLevelAncestor()).dispose(); //closing the current screen.
        });
        
        goButton.addActionListener(e -> {
            ClassPathGUI.createFrame();
            ((JFrame) studentToolsPanel.getTopLevelAncestor()).dispose(); //closing the current screen.
        });
    }

